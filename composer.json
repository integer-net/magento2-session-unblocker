{
  "name": "integer-net/magento2-session-unblocker",
  "description": "Module for Magento 2 that prevents session blocking between concurrent AJAX requests",
  "authors": [
    {
      "name": "Fabian Schmengler",
      "email": "fs@integer-net.de"
    },
    {
      "name": "Willem Wigman",
      "email": "ww@integer-net.de"
    }
  ],
  "require": {
    "magento/framework": "^100.1|^101.0|^102.0",
    "magento/module-customer": "^101.0.0|^102.0.0",
    "magento/module-catalog": "^101.0.0|^102.0.0|^103.0",
    "php": ">=7.1.0"
  },
  "type": "magento2-module",
  "license": [
    "MIT"
  ],
  "autoload": {
    "files": [
      "src/registration.php"
    ],
    "psr-4": {
      "IntegerNet\\SessionUnblocker\\Test\\": "tests/",
      "IntegerNet\\SessionUnblocker\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "require-dev": {
    "magento/magento-coding-standard": "@dev"
  }
}
